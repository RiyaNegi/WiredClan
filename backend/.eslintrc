{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "ignorePatterns": [
        "build/**"
    ],
    "rules": {
        "arrow-parens": [
            "error",
            "always"
        ],
        "max-len": 0,
        "import/first": 0
    },
    "env": {
        "jest": true
    }
}